
@article{ WOS:000802825800001,
Author = {Su, Han and Krol, Maarten S. and Hogeboom, Rick J.},
Title = {The role of context in identifying linkages between SDG 2 (food) and SDG
   6 (water)},
Journal = {SUSTAINABILITY SCIENCE},
