@article{ WOS:000322260200008,
Author = {Kanmaz, Evrim Ozkaynak and Ova, Gulden},
Title = {The effective parameters for subcritical water extraction of SDG lignan
   from flaxseed (Linum usitatissimum L.) using accelerated solvent
   extractor},
