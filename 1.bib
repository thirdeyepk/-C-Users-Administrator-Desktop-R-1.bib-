@article{ WOS:000089390800046,
Author = {Muir, AD and Westcott, ND},
Title = {Quantitation of the lignan secoisolariciresinol diglucoside in baked
   goods containing flax seed or flax meal},
Journal = {JOURNAL OF AGRICULTURAL AND FOOD CHEMISTRY},
Year = {2000},
Volume = {48},
Number = {9},
Pages = {4048-4052},
Month = {SEP},
Abstract = {Samples of commercially prepared white, whole wheat, flax, and
   multigrain breads were analyzed by a rapid RP-HPLC method for the
   presence of the lignan secoisolariciresinol diglucoside (SDG). SDG was
   detected only in products containing flax, with concentrations ranging
   from 0.06 to 1.98 mu m/g of DW (19-602 mu M/loaf). Full-fat flax meal,
   powdered aqueous alcohol extracts of flax seed, and SDG were added to a
   white bread mix and baked into leaves in a domestic bread maker.
   Quantitative recovery of SDG from the test breads was observed when SDG
   was added; however, when flax meal or aqueous alcohol extracts were
   added, only 73-75\% of the theoretical yield of SDG was recovered. SDG
   was also detected in commercially prepared flax cookies, bagels, and
   muffins with concentrations ranging from 0.26 to 2.93 mu M/g of DW. The
   extent of grinding of the flax seed was also shown to have a significant
   effect on the recovery of SDG from both flax meal breads and baked
   goods, with extraction of SDG from finely ground samples greater than
   that from course material.},
Publisher = {AMER CHEMICAL SOC},
Address = {1155 16TH ST, NW, WASHINGTON, DC 20036 USA},
Type = {Article},
Language = {English},
