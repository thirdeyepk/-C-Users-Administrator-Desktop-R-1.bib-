@article{ WOS:000536954300002,
Author = {Bouma, Johan and Montanarella, Luca and Evanylo, Gregory},
Title = {The challenge for the soil science community to contribute to the
   implementation of the UN Sustainable Development Goals},
Journal = {SOIL USE AND MANAGEMENT},
