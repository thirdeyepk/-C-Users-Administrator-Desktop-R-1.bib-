@article{ WOS:000513385500001,
Author = {Consolandi, Costanza and Phadke, Himani and Hawley, Jim and Eccles,
   Robert G.},
Title = {Material ESG Outcomes and SDG Externalities: Evaluating the Health Care
   Sector's Contribution to the SDGs},
Journal = {ORGANIZATION \& ENVIRONMENT},
