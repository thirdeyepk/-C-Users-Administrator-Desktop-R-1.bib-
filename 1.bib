@article{ WOS:000687033100001,
Author = {Ghosh, Dipita and Maiti, Subodh Kumar},
Title = {Biochar-assisted eco-restoration of coal mine degraded land to meet
   United Nation Sustainable Development Goals},
Journal = {LAND DEGRADATION \& DEVELOPMENT},
