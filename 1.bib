@article{ WOS:000495698200003,
Author = {Jabbari, Mehdi and Motlagh, Majid Shafiepour and Ashrafi, Khosro and
   Abdoli, Ghahreman},
Title = {Differentiating countries based on the sustainable development
   proximities using the SDG indicators},
