@article{ WOS:000590976000001,
Author = {Coelho da Costa, Bruno Filipe and Ramalho, Andre and Goncalves-Pinho,
   Manuel and Freitas, Alberto},
Title = {Suicide Mortality Rate as a Sustainable Development Goal (SDG): A
   Bibliometric Analysis},
