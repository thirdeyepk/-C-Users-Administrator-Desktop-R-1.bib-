@article{ WOS:000640703800002,
Author = {Mensi, Azza and Udenigwe, Chibuike C.},
Title = {Emerging and practical food innovations for achieving the Sustainable
   Development Goals (SDG) target 2.2},
Journal = {TRENDS IN FOOD SCIENCE \& TECHNOLOGY},
Year = {2021},
