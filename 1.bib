@article{ WOS:000415331600019,
Author = {Neumann, Barbara and Ott, Konrad and Kenchington, Richard},
Title = {Strong sustainability in coastal areas: a conceptual interpretation of
   SDG 14},
Journal = {SUSTAINABILITY SCIENCE},
Year = {2017},
