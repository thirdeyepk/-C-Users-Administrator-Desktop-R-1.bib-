@article{ WOS:000776541600001,
Author = {Zaneti, R. N. and Fernandes, C. V. S. and Etchepare, R. G.},
Title = {Urban water quality in Porto Alegre (Brazil) in light of the sustainable
   development goal (SDG) 6 and the new national sanitation policy},
Journal = {URBAN WATER JOURNAL},
Abstract = {The Law n degrees 14,026 from 15 July 2020, upgrades Brazil's new
   national sanitation framework. Whether this upgrade in the Law is not
   the market sequestering the Brazilian `Blue and brown gold' had been
   pretty debated, however, we prefer to address two research questions:
   what water quality should we expect in Brazil's urban rivers? How should
   the management of urban rivers be planned? In this work, Porto Alegre
