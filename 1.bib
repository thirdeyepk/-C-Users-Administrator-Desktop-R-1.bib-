@article{ WOS:000615627800001,
Author = {Sobczak, Elzbieta and Bartniczak, Bartosz and Raszkowski, Andrzej},
Title = {Implementation of the No Poverty Sustainable Development Goal (SDG) in
   Visegrad Group (V4)},
Journal = {SUSTAINABILITY},
