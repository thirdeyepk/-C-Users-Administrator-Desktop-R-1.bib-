@article{ WOS:000777818300001,
Author = {Costa, Joana and Cancela, Diana and Reis, Joao},
Title = {Neverland or Tomorrowland? Addressing (In)compatibility among the SDG
   Pillars in Europe},
Journal = {INTERNATIONAL JOURNAL OF ENVIRONMENTAL RESEARCH AND PUBLIC HEALTH},
