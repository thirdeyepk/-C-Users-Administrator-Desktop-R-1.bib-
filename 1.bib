@article{ WOS:000596123500001,
Author = {Mwitondi, Kassim S. and Munyakazi, Isaac and Gatsheni, Barnabas N.},
Title = {A robust machine learning approach to SDG data segmentation},
Journal = {JOURNAL OF BIG DATA},
Year = {2020},
