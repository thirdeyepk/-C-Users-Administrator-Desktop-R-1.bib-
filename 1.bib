@article{ WOS:000276810900011,
Author = {Vishvakarma, S. K. and Saxena, A. K. and Dasgupta, S.},
Title = {Analytical Modeling of Symmetric Double Gate (SDG) MOSFET Using 1D
   Schrodinger-Poisson Equation Solution},
Journal = {JOURNAL OF NANOELECTRONICS AND OPTOELECTRONICS},
Year = {2009},
Volume = {4},
Number = {3},
Pages = {353-361},
Month = {DEC},
Abstract = {In this paper, we have carried out an analytical modeling of Symmetric
   Double Gate (SDG) MOSFET using self-consistent solution of 10
   Schrodinger-Poisson equation to evaluate eigenfunction, position of
   inversion layer centroid, areal inversion charge density, threshold
   voltage and drain current Further, in this paper, we have introduced the
   concept of inversion layer centroid and found that it's location lies at
   a distance of one fourth of the sum of channel and oxide thicknessess
   from top and bottom gates The various variations has been described in
   this paper using various device parameters For the purpose of
   verification of our proposed model for the various results, it has been
   compared and contrasted with the reported results and has been found to
   be in good agreement.},
