@article{ WOS:000752206500001,
Author = {Marton, Toth and Jozsef, Janszky},
Title = {INTRACRANIAL EEG MONITORING METHODS},
Journal = {IDEGGYOGYASZATI SZEMLE-CLINICAL NEUROSCIENCE},
