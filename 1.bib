@article{ WOS:000280960300014,
Author = {Vishvakarma, S. K. and Saxena, A. K. and Dasgupta, S.},
Title = {Modeling and Estimation of Drain Current for Dual Metal Gate (Hf/AIN(x))
   and Midgap Symmetric Double Gate (SDG) MOSFET},
Journal = {JOURNAL OF COMPUTATIONAL AND THEORETICAL NANOSCIENCE},
Year = {2010},
Volume = {7},
Number = {10},
Pages = {1941-1947},
Month = {OCT},
Abstract = {In this research paper, we have carried out analytical modeling of drain
   current for Dual Metal Gate (Hf/AIN(x)) and Midgap Symmetric Double Gate
   (SDG) MOSFET structure. The modeling and estimation of drain current has
   been carried out from analytical 2D potential model using boundary value
   approach based on the physics of the device. This 2D potential modeling
   has been carried out initially for ASymmetric Double Gate (ASDG) MOSFET
   structure in a general way followed by Symmetric Double Gate (SDG)
   MOSFET structure. Further, in this work quantum mechanical correction is
   applied in threshold voltage through the structural confinement
   properties of device. For the accurate estimation of drain current we
   have seen the impact of channel thickness on gate capacitance and
   mobility to estimate the accurate drain current. For the purpose of
   verification and validation of model, we have compared and contrasted
   the results obtained through our model and the results obtained through
   2D ATLAS device simulator.},
