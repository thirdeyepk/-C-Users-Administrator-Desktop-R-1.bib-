Author = {Hwang, Chin-Fa and Yin, Tzu-Chiao and Chang, Nai-Yu and Ho, Chi-Tang},
Title = {Efficiency of a SDG-beta-glucosidase from Bacillus altitudinis HK02 for
   the deglycation of glycosides from flaxseeds},
