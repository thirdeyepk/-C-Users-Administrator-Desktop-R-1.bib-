@article{ WOS:000632898100002,
Author = {Carr, Jamie A. and Petrokofsky, Gillian and Spracklen, Dominick V. and
   Lewis, Simon L. and Roe, Dilys and Trull, Nicholas and Vidal, Adriana
   and Wicander, Sylvia and Worthington-Hill, John and Sallu, Susannah M.},
Title = {Anticipated impacts of achieving SDG targets on forests-a review},
