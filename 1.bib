@article{ WOS:000241857000018,
Author = {Hyvarinen, Helena K. and Pihlava, Juha-Matti and Hiidenhovi, Jaakko A.
   and Hietaniemi, Veli and Korhonen, Hannu J. T. and Ryhanen, Eeva-Liisa},
Title = {Effect of processing and storage on the stability of flaxseed lignan
   added to dairy products},
Journal = {JOURNAL OF AGRICULTURAL AND FOOD CHEMISTRY},
Year = {2006},
Volume = {54},
Number = {23},
Pages = {8788-8792},
Month = {NOV 15},
Abstract = {This study investigated the effects of processing and storage on the
   stability of purified, flaxseed-derived secoisolariciresinol diglucoside
   ( SDG) added to milk prior to the manufacture of different dairy
   products. We analyzed the effect of high-temperature pasteurization,
   fermentation, and milk renneting as well as storage on the stability of
   SDG added to milk, yogurt, and cheese. Also, the stability of SDG in
   whey-based drinks was studied. Added SDG was found to withstand the
   studied processes well. In edam cheese manufacture, most of the added
   SDG was retained in the whey fraction and 6\% was found in the cheese
   curd. SDG was also relatively stable in edam cheese during ripening of 6
   weeks at 9 degrees C and in yogurt during storage of 21 days at 4
   degrees C. Up to 25\% of added SDG was lost in whey-based drinks during
   storage of 6 months at 8 degrees C. We conclude that SDG can be
   successfully supplemented in dairy-based products.},
