@article{ WOS:000659019900002,
Author = {Gjorgievski, Vladimir Z. and Mihajloska, Emilija and Abazi, Alajdin and
   Markovska, Natasha},
Title = {Sustainable Development Goals-Climate Action Nexus:Quantification of
   Synergies and Trade-offs},
Journal = {CLEAN TECHNOLOGIES AND ENVIRONMENTAL POLICY},
