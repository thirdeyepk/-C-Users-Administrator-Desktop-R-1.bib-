@article{ WOS:000541588200001,
Author = {Ferguson, Therese and Roofe, Carmel Geneva},
Title = {SDG 4 in higher education: challenges and opportunities},
Journal = {INTERNATIONAL JOURNAL OF SUSTAINABILITY IN HIGHER EDUCATION},
