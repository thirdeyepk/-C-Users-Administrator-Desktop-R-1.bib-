@article{ WOS:000647385400001,
Author = {Hashim, Mazlan and Sani, Dalhatu Aliyu and Yahya, Nurul Nadiah},
Title = {Appraisal of seagrass aboveground biomass changes using satellite data
   within the tropical coastline of Peninsular Malaysia},
Journal = {GEOCARTO INTERNATIONAL},
