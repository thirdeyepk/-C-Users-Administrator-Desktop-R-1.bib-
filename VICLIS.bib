@article{ WOS:000519846500090,
Author = {Sampantamit, Tiptiwa and Ho, Long and Van Echelpoel, Wout and Lachat,
   Carl and Goethals, Peter},
Title = {Links and Trade-Offs between Fisheries and Environmental Protection in
   Relation to the Sustainable Development Goals in Thailand},
Journal = {WATER},
