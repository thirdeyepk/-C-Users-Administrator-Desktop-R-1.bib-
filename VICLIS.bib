@article{ WOS:000545067300002,
Author = {Gue, Ivan Henderson V. and Ubando, Aristotle T. and Tseng, Ming-Lang and
   Tan, Raymond R.},
Title = {Artificial neural networks for sustainable development: a critical
   review},
